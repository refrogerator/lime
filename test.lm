-- data List a = Cons a (List a) | Nil
data List a = Cons a (List a) | Nil

-- funny n = case n of
--     Cons i l -> i + funny l
--     Nil -> 0
funny :: List Int -> Int
funny n = case n of
    Cons i l -> i + funny l
    Nil -> 0

